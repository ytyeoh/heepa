{{- $input := . -}}
{{- $decoded := $input | replace "%20" " " | replace "%21" "!" | replace "%22" "\"" | replace "%23" "#" | replace "%24" "$" | replace "%25" "%" | replace "%26" "&" | replace "%27" "'" | replace "%28" "(" | replace "%29" ")" | replace "%2C" "," | replace "%2D" "-" | replace "%2E" "." | replace "%2F" "/" | replace "%3A" ":" | replace "%3B" ";" | replace "%3C" "<" | replace "%3D" "=" | replace "%3E" ">" | replace "%3F" "?" | replace "%40" "@" | replace "%5B" "[" | replace "%5D" "]" -}}
{{- return $decoded -}}
